; BIND data file for asa.br zone
$TTL    604800
@       IN      SOA     ns.asa.br. root.asa.br. (
                        2           ; Serial (aumente o valor a cada alteração)
                        604800      ; Refresh
                        86400       ; Retry
                        2419200     ; Expire
                        604800 )    ; Negative Cache TTL

; Nameserver
@       IN      NS      ns.asa.br.

; Registros A (endereços IPv4 para os domínios)
ns              IN      A       192.168.1.67
@               IN      A       192.168.1.67
www             IN      A       192.168.1.67
brito           IN      A       192.168.1.67
matheus         IN      A       192.168.1.67
bezerra         IN      A       192.168.1.67
